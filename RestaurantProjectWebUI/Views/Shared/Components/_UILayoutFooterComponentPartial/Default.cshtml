@model List<ResultContactDto>
@foreach (var item in Model)
{
    <footer class="footer_section">
        <div class="container">
            <div class="row">
                <div class="col-md-4 footer-col">
                    <div class="footer_contact">
                        <h4>
                            Bize Ulaşın
                        </h4>
                        
                            <p>
                                <i class="fa fa-map-marker" aria-hidden="true"></i>
                                <span>
                                    @item.Location 
                                </span>
                            </p>
                            <p>
                                <i class="fa fa-phone" aria-hidden="true"></i>
                                <span>
                                    @item.Phone
                                </span>
                            </p>
                            <p>
                                <i class="fa fa-envelope" aria-hidden="true"></i>
                                <span>
                                    @item.Mail
                                </span>
                            </p>
                        
                    </div>
                </div>
                <div class="col-md-4 footer-col">
                    <div class="footer_detail">
                        <a href="/Default/Index" class="footer-logo">

                            @item.FooterTitle
                        </a>
                        <p>
                            @item.FooterDescription
                        </p>
                        @await Component.InvokeAsync("_UILayoutSocialMediaComponentPartial")
                    </div>
                </div>
                <div class="col-md-4 footer-col">
                    <h4>
                        @item.OpenDays

                    </h4>
                    <p>
                        @item.OpenDaysDescription
                    </p>
                    <p>
                        @item.OpenHours
                    </p>
                </div>
            </div>
            <div class="footer-info">
                <p>
                    &copy; <span id="displayYear"></span> Tüm Hakları Saklıdır.<br>
                    <a href="">Efe's Restaurant</a><br><br>
                    &copy; <span id="displayYear"></span> Geliştiren
                    <a href="">REMZİ EFE YILMAZ</a>
                </p>
            </div>
        </div>
    </footer>
}